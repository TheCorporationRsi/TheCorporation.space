{
    "title": "structures",
    "description": "Default structure of the CORP",
    "endpoints": [
        {
            "name": "User",
            "url": "https://TheCorporation.space/api/v0/users",
            "description": "User endpoint",
            "default_return": "Return a list of all the users",
            "searchable_attributes": [
                {
                    "name": "id",
                    "type": "uuid4",
                    "example": "ABCD-EFGH-IJKL-MNOP",
                    "description": "The unique identifier for the user."
                },
                {
                    "name": "RSI_handle",
                    "type": "string",
                    "example": "CyberDreamer",
                    "description": "The unique Star Citizen handle of a player"
                },
                {
                    "name": "discord_id",
                    "type": "int",
                    "example": "12345678910",
                    "description": "The unique discord id of a user"
                }
            ]
        },
        {
